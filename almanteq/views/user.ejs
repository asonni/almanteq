<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Almanteq</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- Le styles -->
    <link href="css/bootstrap.css" rel="stylesheet">
    <link href="css/bootstrap-editable.css" rel="stylesheet">
    <link href="css/app.css" rel="stylesheet">  
    <link href="css/bootstrap-responsive.css" rel="stylesheet">

    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="../assets/js/html5shiv.js"></script>
    <![endif]-->
  </head>
  <body>
    <div class="container">
      <div class="masthead">
         <div class="row">
          <div class="span6">
            <br>
            <ul class="nav nav-pills">
              <li><a href="/offers">المشتريات</a></li>
              <li><a href="/sales">المبيعات</a></li>
              <li><a href="/users">المستخدمين</a></li>
              <li><a href="/customers">الزبائن</a></li>
              <li><a href="/stock">المخزن</a></li>
              <li class="dropdown active">
                <a class="dropdown-toggle" data-toggle="dropdown" ref="#"> <div id="user-target"></div></a>
                <ul class="dropdown-menu">
                  <li class="active"><a href="/user">حسابي</a></li>
                  <li><a href="/logout">خروج</a></li>
                </ul>
              </li>
            </ul>
          </div>
          <div class="row-fluid">
            <h3 class="muted pull-right"><img id="ownerLogo" src="/img/logo.jpg" class="logo"> Almanteq</h3>
          </div>
        </div>
        <div class='notifications top-right'>
        </div>
        <div class="container-fluid">
          <div class="row-fluid">
            <div class="well">
              <div id="user1-target">
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    </div>
    <!-- Le javascript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script id="user-template" class="tmpl" type="text/x-handlebars-template">
      {{name}}<b class="caret"></b>    
    </script>
    <script id="user1-template" class="tmpl" type="text/x-handlebars-template">
      <legend>My Account / حسابي&nbsp;&nbsp;&nbsp;&nbsp;<button id="enable" class="btn btn-primary "><i class="icon-pencil icon-white"></i> Edit / تعديل</button></legend>
      <table  id="user" class="table table-bordered table-striped" style="clear: both">
        <tbody>
          <tr><td width="20%">الإسم</td><td width="80%"><a href="#" id="name" data-type="text" data-pk="{{user.iduser}}" data-original-title="إسم المستخدم">{{user.name}}</a></td></tr>
          <tr><td>البريد الإلتكتروني</td><td><a href="#" id="email" data-type="text" data-pk="{{user.iduser}}" data-original-title="البريد الإلكتروني">{{user.email}}</a></td></tr>
          <tr><td>كلمة المرور</td><td><a href="#" id="password" data-type="password" data-pk="{{user.iduser}}" data-original-title="كلمة المرور">**********</a></td></tr>
        </tbody>
      </table>
    </script>
    <script src="js/jquery-1.10.1.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <script src="js/bootstrap-editable.js"></script>
    <script src="js/handlebars.js"></script>
    <script src="js/purl.js"></script>
    <script src="js/helpers.js"></script>
    <script src="js/user.js"></script>
    <script src="js/bootstrap-notify.js"></script>
  </body>
</html>
